<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.simplyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SimplyApp">
        <activity android:name=".WeatherActivity"></activity>
        <activity android:name=".MeditationActivity" />
        <activity android:name=".BulletJournalActivity" />
        <activity
            android:name=".MediationAcitvity"
            android:label="@string/title_activity_mediation_acitvity"
            android:theme="@style/Theme.SimplyApp.NoActionBar" />
        <activity
            android:name=".to_do_Page"
            android:label="to_do_page"
            android:parentActivityName=".MenuActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.twoactivites.MenuActivity" />
        </activity>
        <activity
            android:name=".MenuActivity"
            android:label="@string/menuActivity_name"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.twoactivites.MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>